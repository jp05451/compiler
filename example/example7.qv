fun main () {
	var vi1: real[5] = {5.0, 3.0, 4.0, 1.0, 2.0}; // vi1[0]~vi1[4]
	var vi2: real[5] = {2.0, -2.0, 4.0}; // Missing dimensions assumed 0s
	print( vi1 * vi2 ); // Inner product, output: "20"
	print( "\n" );
	print( vi1 + vi2 ); // Dimension-wise addition, output: "{7, 1, 8, 1, 2}"
	print( "\n" );
}